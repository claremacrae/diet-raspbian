# For more options and information see
# http://www.raspberrypi.org/documentation/configuration/config-txt.md
# Some settings may impact device functionality.

# Overclocking settings. Leave things be for maximum stability.
arm_freq={{ config_arm_freq }}
core_freq={{ config_core_freq }}
sdram_freq={{ config_sdram_freq }}
over_voltage={{ config_over_voltage }}

# GPU memory split.
gpu_mem={{ config_gpu_mem }}

# Whether to allow full 1.2A current over USB.
max_usb_current={{ config_max_usb_current }}
